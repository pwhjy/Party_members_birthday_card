/* pages/index/index.wxss */
.background{
  width: 100%;
  height: 100%;
  background-size: 100% 100%;
  z-index: -1;/*图片置于底层，不会影响其他部分*/
  display: block;
  align-items: center;
}
.btn_box{
  display: flex;
  width: 100%;
  height: 100%;
  align-items: center;
  justify-content: center;  
  text-align: center；
}
.primary{
  font-size: 39rpx;
  background: rgb(189, 30, 27);
  position: absolute;
  top: 56%;
  display: flex;
  width: 40%;
  justify-content: center;
  align-content: center;
  align-items: center;
  font-family: 'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif;
  color: rgb(255,215,185);
  border: 1px solid	#F7BF65; 
  border-radius: 1px; 
}
.primary::after{
  border: none;
}